[Desktop Entry]
Type=Application
Exec=/usr/bin/chromium-browser --password-store=basic --noerrdialogs --disable-session-crashed-bubble --disable-infobars --incognito --kiosk {{URL}}
Hidden=false
X-GNOME-Autostart-enabled=true
Name[en_US]=AutoChromium
Name=AutoChromium
Comment=Start Chromium when GNOME starts
